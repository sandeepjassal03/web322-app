<div class="row">
    <div class="col-md-12 tablePosts text-center">
        <br>
        <h2 class="text-orange">{{session.user.userName}} ({{session.user.email}}) History</h2>
        <hr>
        <table class="table table-responsive table-hover text-center text-dark">
            <tr>
                <th>Login Date/Time</th>
                <th>Client Information</th>
            </tr>
            {{#each session.user.loginHistory}}
            <tr>
                <td>{{dateTime}}</td>
                <td>{{userAgent}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>